cmake_minimum_required(VERSION 3.13)
project(qslibsdh-download NONE)

include(ExternalProject)
ExternalProject_Add(qslibsdh
    GIT_REPOSITORY https://github.com/l3ger0j/QSLibSDH.git
    GIT_TAG master
    GIT_PROGRESS true
    SOURCE_DIR "${CMAKE_BINARY_DIR}/qslibsdh-src"
    BINARY_DIR "${CMAKE_BINARY_DIR}/qslibsdh-build"
    CONFIGURE_COMMAND ""
    BUILD_COMMAND ""
    INSTALL_COMMAND ""
    TEST_COMMAND ""
)